    {
      "cell_type": "code",
      "source": [
        "from pyspark.ml.feature import StringIndexer\n",
        "\n",
        "categorical_cols = [\"gender\", \"feeding_type\", \"immunizations_done\", \"reflexes_normal\", \"risk_level\"]\n",
        "indexed_cols = [col + \"_indexed\" for col in categorical_cols]\n",
        "\n",
        "indexers = [StringIndexer(inputCol=col, outputCol=col_indexed) for col, col_indexed in zip(categorical_cols, indexed_cols)]\n",
        "\n",
        "from pyspark.ml import Pipeline\n",
        "pipeline = Pipeline(stages=indexers)\n",
        "df_indexed = pipeline.fit(df).transform(df)\n",
        "\n",
        "df_indexed.show()"
      ],
