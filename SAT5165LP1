        "from pyspark.sql.functions import corr\n",
        "\n",
        "# Select the feature columns and the target column\n",
        "feature_cols = [col_name for col_name in df_assembled.columns if col_name not in [\"baby_id\", \"name\", \"date\", \"gender\", \"feeding_type\", \"immunizations_done\", \"reflexes_normal\", \"risk_level\", \"features\", \"risk_level_indexed\"]]\n",
        "target_col = \"jaundice_level_mg_dl\"\n",
        "\n",
        "# Compute Pearson correlation for each feature with the target\n",
        "correlations = {}\n",
        "for feature in feature_cols:\n",
        "    correlation = df_assembled.select(corr(feature, target_col)).collect()[0][0]\n",
        "    correlations[feature] = correlation\n",
        "\n",
        "# Print the correlations\n",
        "for feature, correlation in correlations.items():\n",
        "    print(f\"Pearson correlation between {feature} and {target_col}: {correlation}\")"
      ],
