        "# Remove the correlation of the target with itself\n",
        "if target_col in correlations:\n",
        "    del correlations[target_col]\n",
        "\n",
        "# Sort features by the absolute value of their correlation in descending order\n",
        "sorted_correlations = sorted(correlations.items(), key=lambda item: abs(item[1]), reverse=True)\n",
        "\n",
        "print(\"Features ranked by the strength of their correlation with Jaundice Level:\")\n",
        "for feature, correlation in sorted_correlations:\n",
